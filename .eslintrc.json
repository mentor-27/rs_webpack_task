{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"],
    "prefer-const": "warn",
    "no-var": "error",
    "no-unused-vars": "warn",
    "no-param-reassign": "error"
  }
}
